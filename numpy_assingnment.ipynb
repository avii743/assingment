      "source": [
        "def moving_avg (x,n=3):\n",
        "  cum_sum = np.cumsum(x,dtype=float)\n",
        "  cum_sum[n:]= cum_sum[n:] - cum_sum[:-n]\n",
        "  return cum_sum[n-1:] / n\n",
        "\n",
        "x = [3, 5, 7, 2, 8, 10, 11, 65, 72, 81, 99, 100, 150]\n",
        "print(moving_avg(x,n=3))"
      ]
